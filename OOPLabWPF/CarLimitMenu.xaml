        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOPLabWPF"
        mc:Ignorable="d"
        Title="Set Car Limit Menu" Height="128" Width="326" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" Icon="/Icon/car-icon-png-4272.png">
    <Grid>
        <Label Content="How many cars program can handle?" HorizontalAlignment="Left" Margin="8,12,0,0" VerticalAlignment="Top" Width="220" FontWeight="Bold"/>
        <Button Content="Submit" HorizontalAlignment="Left" Margin="234,48,0,0" VerticalAlignment="Top" Height="24" Width="66" Click="Button_Click"/>
        <TextBox x:Name="TextBoxValue" HorizontalAlignment="Left" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="208" Margin="14,52,0,0" PreviewTextInput="TextBox_PreviewTextInput" PreviewKeyDown="TextBoxValue_PreviewKeyDown"/>

    </Grid>
